{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ESNext", "Decorators", "DOM", "DOM.Iterable"],
    "module": "Preserve",
    "target": "ES2022",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    // "experimentalDecorators": true,
    "useDefineForClassFields": false,
  }
}
