{
  "name": "image-viewer",
  "displayName": "image-viewer",
  "description": "image-viewer",
  "version": "0.0.1",
  "publisher": "faraday",
  "author": "Mikhail Isupov",
  "license": "MIT",
  "private": true,
  "contributes": {
    "quickViews": [
      {
        "id": "img",
        "extensions": [
          ".apng",
          ".avif",
          ".gif",
          ".jpg",
          ".jpeg",
          ".jfif",
          ".pjpeg",
          ".pjp",
          ".png",
          ".svg",
          ".webp",
          ".bmp",
          ".ico"
        ],
        "mimetypes": [
          "image/apng",
          "image/avif",
          "image/gif",
          "image/jpeg",
          "image/png",
          "image/svg+xml",
          "image/webp",
          "image/bmp",
          "image/x-icon"
        ],
        "path": "./image.js"
      }
    ]
  }
}
