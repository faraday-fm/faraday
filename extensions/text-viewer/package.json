{
  "name": "text-viewer",
  "displayName": "Text Viewer",
  "publisher": "faraday",
  "description": "Simple text viewer",
  "author": "Mikhail Isupov",
  "version": "1.2.0",
  "license": "MIT",
  "private": true,
  "categories": [
    "QuickView"
  ],
  "keywords": [
    "quickview",
